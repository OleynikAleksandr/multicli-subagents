{
    "name": "multicli-agents",
    "displayName": "SubAgent Manager",
    "description": "Manage CLI Sub-Agents inside VS Code",
    "version": "0.0.1",
    "publisher": "OleksandrOliinyk",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "multicli-agents.open",
                "title": "SubAgent Manager: Open"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "npx ultracite check",
        "format": "npx ultracite fix",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.85.0",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "20.2.5",
        "typescript": "^5.1.3",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "@vscode/test-electron": "^2.3.2",
        "ultracite": "latest"
    }
}